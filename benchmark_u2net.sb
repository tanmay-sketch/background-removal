#!/bin/bash --login
#SBATCH --job-name=benchmark_u2net
#SBATCH --output=benchmark_u2net_%j.out
#SBATCH --time=04:00:00
#SBATCH --nodes=1
#SBATCH --ntasks=1
#SBATCH --cpus-per-task=64
#SBATCH --mem=8G
#SBATCH --partition=general-short
#SBATCH --gpus=v100:1

# Load Miniforge3
module purge
module load Miniforge3

# Activate conda environment
conda activate rembg_env

# Set OpenMP thread count
export OMP_NUM_THREADS=64

# Run the benchmark script
python u2net/benchmark.py u2net