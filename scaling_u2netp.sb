#!/bin/bash --login
#SBATCH --job-name=scaling_u2netp
#SBATCH --output=scaling_u2netp_%j.out
#SBATCH --time=04:00:00
#SBATCH --nodes=1
#SBATCH --ntasks=1
#SBATCH --cpus-per-task=64
#SBATCH --mem=8G

# Load Miniforge3
source /mnt/home/tanmay/miniforge3/etc/profile.d/conda.sh

# Activate conda environment
conda activate rembg_env

# Set OpenMP thread count
export OMP_NUM_THREADS=64

# Run the scaling benchmark script
python u2net/scaling_benchmark.py u2netp