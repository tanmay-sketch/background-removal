#!/bin/bash --login
#SBATCH --job-name=scaling_u2net
#SBATCH --output=scaling_u2net_%j.out
#SBATCH --time=04:00:00
#SBATCH --nodes=1
#SBATCH --ntasks=1
#SBATCH --cpus-per-task=32
#SBATCH --mem=8G

cd $SLURM_SUBMIT_DIR

module purge
module load Miniforge3
conda activate rembg_env

# Set OpenMP threads to match CPU count
export OMP_NUM_THREADS=$SLURM_CPUS_PER_TASK

# Run the benchmark
python benchmarking/scaling_benchmark.py u2net